@model MYUSIS.Models.StudentNote
@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_LayoutPageAdmin.cshtml";
}
<div class="alert alert-danger" role="alert" style="margin-left: 25px; margin-right: 25px;">
    <p><h5 style="text-align: center;"><b>Öğrenci Not Ortalaması Silme Bölümündesiniz</b></h5></p><hr />
    <p style="text-align: center;">Sileceğiniz Öğrenci Not Ortalaması Bilgileri Aşağıdaki Gibidir</p>
</div>

<div class="card shadow mb-4" style="margin-left: 25px; margin-right: 25px;">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Öğrenci Not Ortalaması</h6>
    </div>
    <div class="card-body">
        <div><b>Adı Soyadı: </b>@Html.DisplayFor(model => model.Student.FullName)</div>
        <div><b>AGNO/Kredi: </b>@Html.DisplayFor(model => model.Agno)/<b>@Html.DisplayFor(model => model.AgnoKredi)</b></div>
        <div><b>YANO/Kredi: </b>@Html.DisplayFor(model => model.Yano)/<b>@Html.DisplayFor(model => model.YanoKredi)</b></div>
        <div><b>ÇAD YANO/Kredi: </b>@Html.DisplayFor(model => model.CadYano)/<b>@Html.DisplayFor(model => model.CadYanoKredi)</b></div>
    </div>
</div>


<div class="col-md-offset-2 col-md-10">

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="col-md-offset-2 col-md-10" style="margin-left: 10px;">
            <input type="submit" value="Sil" class="btn btn-danger" />
            <a class="btn btn-warning" href="/StudentNotes/Index" role="button">Geri Dön</a>
        </div>
    }
</div>

