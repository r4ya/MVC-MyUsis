@model MYUSIS.Models.FinishTime

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutPageAdmin.cshtml";
}

<div class="alert alert-warning" role="alert" style="margin-left: 25px; margin-right: 25px;">
    <p><h5 style="text-align: center;"><b>Bitiş Saati Bilgi Bölümündesiniz</b></h5></p><hr />
    <p style="text-align: center;">Seçtiğiniz Bitiş Saati Bilgileri Aşağıdaki Gibidir.</p>
</div>

<div style="margin-left: 25px; margin-right: 25px;">
    <div class="card">
        <div class="card-header">
            <b>Ders Başlangıç Saati: </b>@Html.DisplayFor(model => model.FinishTime1)
        </div>
    </div>
</div>
<br />
<div class="col-md-offset-2 col-md-10" style="margin-left: 15px;">
    <button class="btn btn-warning text-white">
        @Html.ActionLink("Düzenle", "Edit", new { id = Model.FinishTimeID }, new { @class = "btn-sm btn-warning" })
    </button>
    <a class="btn btn-info" href="/FinishTimes/Index" role="button">Geri Dön</a>
</div>

